

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
  	<description></description>
  	<servlet-class>controller.CustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>controller.LoginServlet</servlet-class>
  	<init-param>
      <param-name>akash</param-name>
      <param-value>patel</param-value>
    </init-param>
    <init-param>
      <param-name>Hemanth</param-name>
      <param-value>pass2</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/CustomerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <taglib>
      <taglib-uri>coretag</taglib-uri>
      <taglib-location>tlds/c.tld</taglib-location>
    </taglib>
    <taglib>
      <taglib-uri>formattag</taglib-uri>
      <taglib-location>tlds/fmt.tld</taglib-location>
    </taglib>
  </jsp-config>
</web-app>
